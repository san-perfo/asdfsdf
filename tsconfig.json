{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "rootDir": "src",
    "watch": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "*": [
        "types/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": ["node", "express"],
  }
}